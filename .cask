# Install native apps
brew tap phinze/homebrew-cask
brew install brew-cask

function installcask() {
	brew cask install "${@}" 2> /dev/null
}

installcask sublime-text
installcask install spotify
installcask install alfred
installcask alfred link
installcask install google-chrome
installcask install dropbox
installcask install evernote
installcask install f-lux
installcask install iterm2
installcask install virtualbox
installcask install vlc
installcask install transmission
installcask install firefox
installcask install the-unarchiver
installcask install skype
